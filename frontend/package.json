          }
        }
      ],
      "@babel/react"
    ]
  },
  "dependencies": {
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "url-loader": "^4.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.7.4",
    "@babel/core": "7.7.4",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "@babel/preset-react": "7.7.4",
    "autoprefixer": "^9.8.0",
    "babel-jest": "24.9.0",
    "concurrently": "^5.2.0",
    "jest": "24.9.0",
    "jsdom": "15.2.1",
    "postcss-cli": "^7.1.1",
    "react-addons-test-utils": "^15.6.2",
    "tailwindcss": "^1.4.6"
  },
  "scripts": {
    "start": "npm run watch:css && react-scripts start",
    "build": "npm run build:css && react-scripts build",
